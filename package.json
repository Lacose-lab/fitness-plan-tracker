{
  "name": "fitness-plan-tracker",
  "private": true,
  "version": "0.2.3",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "build:gh": "cross-env GITHUB_PAGES_BASE=/$npm_package_name/ npm run build",
    "lint": "eslint .",
    "preview": "vite preview",
    "preview:ci": "vite preview --port 4173 --strictPort",
    "test:e2e": "playwright test --reporter=line",
    "playwright:install": "playwright install",
    "qa": "npm run build && npm run playwright:install && start-server-and-test preview:ci http://127.0.0.1:4173 test:e2e",
    "gen:icons": "node scripts/gen-icons.mjs"
  },
  "dependencies": {
    "chart.js": "^4.5.1",
    "date-fns": "^4.1.0",
    "react": "^19.2.0",
    "react-chartjs-2": "^5.3.1",
    "react-dom": "^19.2.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@playwright/test": "^1.58.0",
    "@types/node": "^24.10.1",
    "@types/react": "^19.2.5",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.1",
    "cross-env": "^10.1.0",
    "eslint": "^9.39.1",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.24",
    "globals": "^16.5.0",
    "sharp": "^0.34.5",
    "start-server-and-test": "^2.1.3",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.46.4",
    "vite": "^7.2.4",
    "vite-plugin-pwa": "^1.2.0"
  }
}
